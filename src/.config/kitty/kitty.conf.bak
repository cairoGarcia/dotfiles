# Configurações gerais
font_size       20.0
font_style bold
# Ativar true color
true_color      yes

# Definir a cor de fundo e texto

# Habilitar transparência
background_opacity 0.84
# Definir comportamento do scroll
scrollback_lines 10000

window_padding_width 0


# BEGIN_KITTY_FONTS
font_family      family="Relaxed Typing Mono JP"
bold_font        auto
italic_font      auto
bold_italic_font auto
# END_KITTY_FONTS
# Added by Gogh
include colors.conf
